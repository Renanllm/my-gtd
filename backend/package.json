{
  "name": "my-gtd-backend",
  "version": "1.0.0",
  "description": "Backend API for My GTD application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "seed": "ts-node --transpile-only prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev"
  },
  "keywords": [
    "express",
    "api",
    "nodejs",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "better-auth": "^1.2.10",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.19.1",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.2",
    "prisma": "^6.10.1",
    "supertest": "^7.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  }
}